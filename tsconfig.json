{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "baseUrl": ".",
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./dist",
      "rootDirs": ["./src"],
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "skipLibCheck": true
   },
   "include": ["./src/**/*.ts"],
   "exclude": ["./node_modules/*", "dist", "client"],

}